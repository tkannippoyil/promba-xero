<%- model_class = Setting -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:consumer_key) %>:</strong></dt>
  <dd><%= @setting.consumer_key %></dd>
  <dt><strong><%= model_class.human_attribute_name(:consumer_secret) %>:</strong></dt>
  <dd><%= @setting.consumer_secret %></dd>
  <dt><strong><%= model_class.human_attribute_name(:prompa_url) %>:</strong></dt>
  <dd><%= @setting.prompa_url %></dd>
  <dt><strong><%= model_class.human_attribute_name(:prompa_token) %>:</strong></dt>
  <dd><%= @setting.prompa_token %></dd>
  <dt><strong><%= model_class.human_attribute_name(:api_token) %>:</strong></dt>
  <dd><%= @setting.api_token %></dd>
</dl>

<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_settings_path, :class => 'btn btn-default' %>

